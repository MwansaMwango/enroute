{
  "name": "enroute",
  "homepage": "https://github.com/MwansaMwango/enroute",
  "version": "1.0.0",
  "description": "Efficient transport for your business",
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "cd database/seeds && node index.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "Copyright retained",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@date-io/date-fns": "^2.8.0",
    "@date-io/moment": "^1.3.13",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@pusher/push-notifications-server": "^1.0.2",
    "axios": "^0.18.1",
    "bcrypt": "^5.0.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "create-react-context": "^0.3.0",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "express-session": "^1.17.1",
    "faker": "^4.1.0",
    "if-env": "^1.0.4",
    "materialize-css": "^1.0.0",
    "moment": "^2.27.0",
    "moment-timezone": "^0.5.31",
    "mongoose": "^5.9.27",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "react-material-ui": "0.0.1",
    "react-materialize": "^3.9.2",
    "react-moment": "^0.9.7",
    "react-slick": "^0.27.10",
    "validator": "^13.1.1"
  }
}
